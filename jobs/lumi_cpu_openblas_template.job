#!/bin/bash
#SBATCH --partition=standard-g
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --gpus-per-node=1
#SBATCH --time=12:00:00
#SBATCH --account=project_462000397
#SBATCH --exclusive

set -eu
date
pwd 
nproc

set -o xtrace

export OPENBLAS_NUM_THREADS=$NPROC 
export OMP_PROC_BIND=spread 
export OMP_PLACES=cores 
./cpu-blob -i "$ITER" -d "$SIZE" --no_gpu -o "CSV_lumi_cpu_openblas_i${ITER}_d${SIZE}_${OPENBLAS_NUM_THREADS}t_${OMP_PROC_BIND}_${OMP_PLACES}"